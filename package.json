{
  "name": "next-to-do-list",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build-ts": "tsc -p tsconfig.next.json && tsc -p tsconfig.server.json",
    "build-static": "npx copyfiles -u 1 \"src/static/**/*\" dist",
    "build-next": "next build ./dist",
    "build": "yarn build-ts && yarn build-static && yarn build-next",
    "start-node": "cd ./dist && node server",
    "start-nodemon": "cd ./dist && nodemon server --watch server",
    "start": "yarn build-ts && yarn build-static && yarn start-node",
    "dev": "yarn build-ts && yarn build-static && yarn start-nodemon",
    "tslint": "tslint -c tslint.next.json -p tsconfig.next.json && tslint -c tslint.server.json -p tsconfig.server.json"
  },
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "antd": "^3.13.2",
    "errorhandler": "^1.5.0",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "express-validator": "^5.3.1",
    "graphql": "^14.1.1",
    "isomorphic-unfetch": "^3.0.0",
    "js-base64": "^2.5.1",
    "mongoose": "^5.4.12",
    "morgan": "^1.9.1",
    "next": "^8.0.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-router": "^4.3.1",
    "tslint": "^5.12.1"
  },
  "devDependencies": {
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.16.1",
    "@types/express-graphql": "^0.6.2",
    "@types/js-base64": "^2.3.1",
    "@types/mongoose": "^5.3.16",
    "@types/morgan": "^1.7.35",
    "@types/next": "^8.0.1",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.1",
    "@types/react-router": "^4.4.4",
    "babel-plugin-import": "^1.11.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "nodemon": "^1.18.10",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3"
  },
  "license": "MIT"
}
